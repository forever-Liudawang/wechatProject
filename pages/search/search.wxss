page{
    background-color: rgb(240, 233, 233);
}
.searchContainer{
    padding: 0 20rpx;
}
.search{
    display: block;
    margin-bottom: 70rpx;
}
.title{
    margin-bottom: 10rpx;
}
.titleBox{
    background-color: #fff;
    padding: 30rpx 14rpx;
}
.title::before{
    display: block;
    content: "";
    width: 100%;
    height: 3rpx;
    background-color: rgb(121, 117, 117);
    margin-bottom: 45rpx;
}
.info::after{
    display: block;
    content: "";
    width: 100%;
    height: 3rpx;
    background-color: rgb(121, 117, 117);
    margin-top: 45rpx;

}
.mation{
    color: rgb(141, 131, 131);
    font-size: 40rpx;
}

